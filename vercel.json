{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "server.js" }
  ],
  "build": {
    "commands": [
      "npm install",
      "npm install puppeteer --unsafe-perm=true"
    ],
    "env": {
      "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "false",
      "PUPPETEER_EXECUTABLE_PATH": "/usr/bin/chromium"
    }
  }
}
